{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native@~0.74.0",
        "react-native-reanimated@~3.10.0",
        "react-native-gesture-handler@~2.16.1",
        "react-native-screens@~3.31.1"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true
      }
    }
  },
  "scripts": {
    "start": "EXPO_UNSTABLE_CORE_AUTOLINKING=1 npx expo start -c",
    "android": "npx expo run:android",
    "ios": "EXPO_UNSTABLE_CORE_AUTOLINKING=1 npx expo run:ios",
    "eject": "npx expo eject",
    "fix-expo-deps": "npx expo install --check",
    "xcode": "xed -b ios",
    "update:source-maps-sentry": "npx sentry-expo-upload-sourcemaps dist",
    "prebuild": "npx expo prebuild --clear"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.6",
    "@expo/config-plugins": "~8.0.4",
    "@gorhom/bottom-sheet": "^4",
    "@my/ui": "0.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@sentry/react-native": "^5.31.1",
    "@shopify/react-native-skia": "1.2.3",
    "@tamagui/animations-moti": "^1.112.0",
    "app": "0.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "burnt": "^0.12.2",
    "color": "^4.2.3",
    "expo": "~51.0.31",
    "expo-blur": "~13.0.2",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.25",
    "expo-font": "~12.0.6",
    "expo-haptics": "~13.0.1",
    "expo-image": "~1.12.15",
    "expo-image-picker": "~15.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-sensors": "~13.0.9",
    "expo-sharing": "~12.0.1",
    "expo-speech": "~12.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-task-manager": "~11.8.2",
    "expo-updates": "~0.25.24",
    "expo-video": "^1.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "~0.75.0",
    "react-native-gesture-handler": "~2.18.1",
    "react-native-reanimated": "~3.15.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.34.0",
    "react-native-svg": "15.2.0",
    "react-native-vision-camera": "^4.5.2",
    "react-native-web": "~0.19.12",
    "ts-pattern": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@expo/metro-config": "~0.18.4",
    "@tamagui/babel-plugin": "^1.112.0",
    "@types/color": "^3.0.6",
    "metro-minify-terser": "^0.80.9",
    "typescript": "~5.3.3"
  },
  "resolutions": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  },
  "overrides": {
    "metro": "0.80.9",
    "metro-resolver": "0.80.9"
  }
}
